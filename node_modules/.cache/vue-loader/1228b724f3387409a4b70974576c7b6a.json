elivery order theo ID\"\n                prefixIcon=\"search\"\n                class=\"mb-2\"\n                type=\"username\"\n                :value=\"filter.search\"\n                @keyup.enter=\"handleSearch\"\n              >\n                <template slot=\"append\">\n                  <p-button\n                    type=\"default\"\n                    icon=\"close\"\n                    v-if=\"filter.search\"\n                    @click=\"clearSearch\"\n                  ></p-button>\n                </template>\n              </p-input>\n              <VclTable class=\"my-20\" v-if=\"isFetching\"> </VclTable>\n              <template v-else-if=\"containers.length\">\n                <div class=\"table-responsive\">\n                  <table class=\"table table-hover\">\n                    <thead>\n                      <tr>\n                        <th width=\"80\">ID</th>\n                        <th>Tổng tiền</th>\n                        <th>Ngày</th>\n                        <th>Trạng thái</th>\n                        <th class=\"text-right\">Chức năng</th>\n                      </tr>\n                    </thead>\n                    <tbody>\n                      <tr v-for=\"(item, index) in containers\" :key=\"index\">\n                        <td width=\"80\">\n                          <router-link\n                            class=\"text-no-underline\"\n                            :to=\"{\n                              name: 'container-view',\n                              params: { id: item.id },\n                            }\"\n                          >\n                            {{ item.id }}\n                          </router-link>\n                        </td>\n                        <td>\n                          <span v-if=\"item.status === 'pending'\">\n                            Pending\n                          </span>\n                          <span v-else>\n                            {{ item.total_amount | format